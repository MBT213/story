<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="category" catalog="story"
		class="com.story.model.Category">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="comments" catalog="story"
		class="com.story.model.Comments">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="novel" catalog="story" class="com.story.model.Novel">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="person" catalog="story" class="com.story.model.Person">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>